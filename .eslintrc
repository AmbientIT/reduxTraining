{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "arrow-body-style": 0,
    "global-require": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "import/prefer-default-export": 0,
    "no-extra-boolean-cast": 0,
    semi: ["error", "never"],
    "arrow-parens": 0,
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "optionalDependencies": false, "peerDependencies": false}]
  }
}
